<!DOCTYPE html>
<html>
<div>  
  <div class="container">
    <div class="row">
      <div class="col-md-5">
        <h1>Upcomming Events</h1>
      </div>
    </div> 
  </div> 
</div>
 
<% @event.each do |s| %>
<div class="container">
  <div class="row">
    <div class="col-md-10">
      <h2><%= 'Upcomming Event: ' %><%= s.name %></h2>
        <div class="info-block">
          <h4><%= 'Event Location: ' %><%= s.location %></h4>
          <h4><%= 'Event Date: ' %><%= s.eventdate %></h4>
          <h4><%= 'Event Description: ' %><%= s.description %></h4>
        </div>
   	</div>
  </div>
</div> 
<% end %>
