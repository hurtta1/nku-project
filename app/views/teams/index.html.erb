<div class="page-header">
  <h1>League Teams<button class="btn btn-default btn-lg pull-right" type="button"><%=link_to "Submit Team Application", new_team_path %></button></h1>
</div>

<% @team.each do |team| %>
  <div id="<%= dom_id(team) %>" class="list-item">
    <div class="info-block">
      <h4><%= team.name %></h4>
      <p><%= team.cname %></p>
    </div>
  </div>
<% end %>